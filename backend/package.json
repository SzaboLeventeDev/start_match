{
  "name": "backend",
  "version": "0.0.1",
  "description": "backend part of SmartMatch application",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "start": "set NODE_ENV=production&& docker compose up",
    "dev": "set NODE_ENV=development&& nodemon --exec ts-node src/index.ts",
    "test": "jest --passWithNoTests --watchAll --verbose",
    "test:cov": "jest --coverage --passWithNoTests",
    "lint": "npx eslint . --ext .js,.ts",
    "lint:fix": "npx eslint . --ext .js,.ts --fix",
    "sequelize": "sequelize init",
    "migration": "sequelize-cli db:migrate",
    "migration:generate": "npx sequelize-cli migration:generate --name",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "migration:generate:seed": "npx sequelize-cli seed:generate --name",
    "migration:data": "npx sequelize-cli db:seed:all"
  },
  "author": "Levente Szab√≥",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-restricted-globals": "^0.2.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.0.3",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
